    "def convert_location_data(x, y) :\n",
    "    '''\n",
    "    lng = a * x1 - b * y2 + c \n",
    "    lat = a * x2 + b * y1 + d\n",
    "\n",
    "    NEMO -> indoor position value\n",
    "    (840, -351) -> (96.824, 0)\n",
    "    (923, -179) -> (107.068, 15.874)\n",
    "    '''\n",
    "    a = 0.1185\n",
    "    b = -0.003\n",
    "    c = -1.81\n",
    "    d = 39.842\n",
    " \n",
    "    lng = a * x - b * y * (-1) + c - 4\n",
    "    lat = a * y * (-1) + b * x + d - 0.5\n",
    "\n",
    "    return lng, lat"
   ]
